      "pluginVersion": "12.3.0",
      "title": "‚è∞ Services Last Seen (Heartbeat)",
      "description": "Time since last metrics received. GREEN = <=25s (active with 15s push), YELLOW = 25-45s (delayed/slow), RED = >45s or NEVER (service down or no traffic). Expected: api-gateway, auth, user, location, trip, logger.",
      "type": "stat",
      "targets": [
        {
          "expr": "time() - max(timestamp(http_server_request_count_total{service_name=\"api-gateway\"}))",
          "refId": "A",
          "legendFormat": "api-gateway"
        },
        {
          "expr": "time() - max(timestamp(rpc_server_duration_milliseconds_count{job=\"authentication-service\"}))",
          "refId": "B",
          "legendFormat": "authentication-service"
        },
        {
          "expr": "time() - max(timestamp(rpc_server_duration_milliseconds_count{job=\"user-service\"}))",
          "refId": "C",
          "legendFormat": "user-service"
        },
        {
          "expr": "time() - max(timestamp(rpc_server_duration_milliseconds_count{job=\"location-service\"}))",
          "refId": "D",
          "legendFormat": "location-service"
        },
        {
          "expr": "time() - max(timestamp(rpc_server_duration_milliseconds_count{job=\"trip-service\"}))",
          "refId": "E",
          "legendFormat": "trip-service"
